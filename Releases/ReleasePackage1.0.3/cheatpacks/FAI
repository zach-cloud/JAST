call MimbA(vfai_e7,GetStoredString(vfai_YauFei,I2S(vfai_i0),I2S(vfai_lo7)))
set vfai_lo7=vfai_lo7+1
exitwhen vfai_lo7>=vfai_aB8
endloop
set vfai_e7=null
endfunction

set XC=BlzGetTriggerSyncData()
set Mr[XD[GetPlayerId(IC)]]=false
if Ar then
set ir=XC
set Pr=IC
set udg_SaveLoadEvent=1.
set udg_SaveLoadEvent=-1.
endif
set IC=null
endfunction
